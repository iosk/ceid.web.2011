<h1>Log in</h1>
<%= form_for(:session, :url => sessions_path) do |f| %>
	
	<div class="field">
		<%= f.label :username %></br>
		<%= f.text_field :username %></br>
	</div>

	<div class="field">
		<%= f.label :password %></br>
		<%= f.password_field :password %></br>
	</div>

	<div class="actions">
    		<%= f.submit "Log In" ,:class => "buttonto blue" %>
 	</div>
<% end %>

<p>Not registered? <%= link_to "Register Here", register_path %></p>
